{
    "require": [
      "ts-node/register"
    ],
    "node-option": [
      "experimental-specifier-resolution=node",
      "loader=ts-node/esm",
      "no-warnings"
    ],
    "extensions": [
      "ts"
    ],
    "spec": [
      "**/test/**/*.spec.ts"
    ],
    "watch-files": [
      "src",
      "modules",
      "examples"
    ],
    "ignore": [
      "**/node_modules/**",
      "**/dist/**"
    ],
    "timeout": "10000"
  }
  